<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ynov.aro.messagemanager.MenuActivity">

    <Button
        android:id="@+id/AddEcole"
        android:layout_width="296dp"
        android:layout_height="135dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="86dp"
        android:text="Ajouter une entrée"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listeEcole" />

    <Button
        android:id="@+id/listeEcole"
        android:layout_width="293dp"
        android:layout_height="135dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="35dp"
        android:text="Liste Entrées"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
